{
  "name": "annota",
  "version": "0.7.3",
  "description": "A React annotation framework for digital pathology and whole slide imaging",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "build:docs": "pnpm build && cd docs && pnpm install && pnpm build",
    "start:docs": "cd docs && pnpm start",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "pathology",
    "annotation",
    "openseadragon",
    "whole-slide-imaging",
    "digital-pathology",
    "react",
    "cell-counting"
  ],
  "author": "BitRoc Lab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitroc-ai/annota.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "peerDependencies": {
    "openseadragon": "^4.0.0 || ^5.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "jsfive": "^0.4.0",
    "pako": "^2.1.0",
    "pixi.js": "^8.14.0",
    "polygon-clipping": "^0.15.7",
    "rbush": "^4.0.1",
    "upng-js": "^2.1.0"
  },
  "devDependencies": {
    "@types/openseadragon": "^3.0.10",
    "@types/pako": "^2.0.4",
    "@types/rbush": "^4.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/ui": "^4.0.2",
    "jsdom": "^27.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.2"
  }
}
